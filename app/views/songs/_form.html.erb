<div class="container">
  <h1>Upload new song</h1>
  <%= simple_form_for(@song) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
        <%= f.input :name %>
        <%= f.input :picture, as: :file %>
        <%= f.input :audio, as: :file %>
      </div>

      <div class="form-actions">
        <%= f.button :submit %>
      </div>
  <% end %>
</div>